<div class="container content">
<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">昵称</th>
      <th scope="col">邮箱</th>
      <th scope="col">手机号</th>
      <th scope="col">操作</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |item|%>
    <tr>
      <th scope="row"><%= item.id %></th>
      <td><%= item.nick_name %></td>
      <td><%= item.email %></td>
      <td><%= item.mobile %></td>
      <td>
      <%= link_to  "编辑", edit_admin_user_path(item.id) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>
</div>